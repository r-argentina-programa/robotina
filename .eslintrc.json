{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "airbnb-typescript/base", "prettier"],
  "plugins": ["@typescript-eslint"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.eslint.json"
  },

  "rules": {
    "import/prefer-default-export": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
